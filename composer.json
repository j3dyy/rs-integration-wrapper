{
  "name": "j3dyy/rs-integration-wrapper",
  "description": "rs.ge integration services project now is under development",
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^11.1",
    "mockery/mockery": "^1.6",
    "pestphp/pest": "3.x-dev",
    "nesbot/carbon": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "J3dyy\\RsIntegrationWrapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "J3dyy\\RsIntegrationWrapper\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "j3dyy",
      "email": "mr.nizharadze@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "./vendor/bin/pest",
    "format": "php-cs-fixer fix --allow-risky=yes",
    "analyse": "phpstan analyse"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
